          "foreground": "#F8F8F2"
      }
  },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#88846f"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "name": "Template Definition",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "name": "Number",
