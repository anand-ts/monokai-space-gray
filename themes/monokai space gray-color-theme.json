                "fontStyle": ""
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "fontStyle": "",
                "foreground": "#F44747"
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
